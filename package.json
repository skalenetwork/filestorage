  "description": "SKALE Filestorage contracts",
  "keywords": [
    "SKALE",
    "blockchain",
    "ethereum"
  ],
  "homepage": "https://github.com/skalenetwork/filestorage#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skalenetwork/filestorage.git"
  },
  "license": "AGPL-3.0",
